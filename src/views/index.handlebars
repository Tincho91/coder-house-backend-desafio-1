    <h1>{{mensaje}} {{user.nombre}}!</h1>

    {{#if isAdmin}}
        <h2>Panel de Administrador</h2>
    {{else}}
        <h2>Panel de Usuario</h2>
    {{/if}}

    <ul>
        {{#each cursos}}
            <li>{{numComision}} - {{dias}} - {{horario}}</li>
        {{/each}}
    </ul>

    {{#if products}}
        {{>productList}}
    {{else}}
        <p>No products to display</p>
    {{/if}}